{
  "containerDefinitions": [{
    "portMappings": [{
      "hostPort": 8000,
      "protocol": "tcp",
      "containerPort": 8000
    }],
    "memory": 300,
    "volumesFrom": [],
    "image": "${IMAGE_NAME}",
    "essential": true,
    "name": "Robobot-Backend"
  }],
  "family": "Robobot-Backend",
  "requiresCompatibilities": [
    "EC2"
  ],
  "networkMode": "awsvpc",
  "volumes": [{
    "name": "Docker-Daemon",
    "host": {
      "sourcePath": "/var/run/docker.sock"
    }
  }]
}