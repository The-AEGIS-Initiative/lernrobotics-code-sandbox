{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": ["src/model", "src/routers", "app.js"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "plugins/markdown"
    ],
     "templates": {
    },
    "opts": {
        "destination": "docs",
        "recurse": true,
        "readme": "./README.md"
    }
}
